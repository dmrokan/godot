<?xml version="1.0" encoding="UTF-8" ?>
<class name="AudioEffectGenerator" inherits="AudioEffect" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../class.xsd">
	<brief_description>
		Adds tone and noise to the audio bus.
	</brief_description>
	<description>
		Adds sine and saw tone generators with adjustable frequency, amplitude and phase. Also, adds white, brown and pink noise generators with adjustable mean and standard deviation.
	</description>
	<tutorials>
	</tutorials>
	<methods>
		<method name="reset">
			<return type="void" />
			<description>
			</description>
		</method>
	</methods>
	<members>
		<member name="damping" type="float" setter="set_damping" getter="get_damping" default="0.0">
		</member>
		<member name="deviation" type="float" setter="set_std" getter="get_std" default="0.1">
		</member>
		<member name="frequency" type="float" setter="set_frequency" getter="get_frequency" default="400.0">
		</member>
		<member name="gain_db" type="float" setter="set_gain_db" getter="get_gain_db" default="0.0">
		</member>
		<member name="mean" type="float" setter="set_mean" getter="get_mean" default="0.0">
		</member>
		<member name="offset" type="float" setter="set_offset" getter="get_offset" default="0.0">
		</member>
		<member name="phase" type="float" setter="set_phase" getter="get_phase" default="0.0">
		</member>
		<member name="sampling_frequency" type="float" setter="set_sampling_frequency" getter="get_sampling_frequency" default="44100.0">
		</member>
		<member name="type" type="String" setter="set_type" getter="get_type" default="&quot;Tone&quot;">
		</member>
	</members>
</class>
